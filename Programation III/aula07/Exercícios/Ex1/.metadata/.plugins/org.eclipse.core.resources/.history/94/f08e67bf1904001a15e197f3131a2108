package aula07Ex1;
import java.io.*;

public class Main {

	public static void main(String[] args) throws IOException{
		Aeroporto aveiroAirlines = new Aeroporto("companhias.txt", "voos.txt");
		System.out.println(aveiroAirlines.toString());
		System.out.println("Exportação da tabela para o ficheiro \"Infopublico.txt\": "+aveiroAirlines.writeToFile("Infopublico.txt"));
	}

}
