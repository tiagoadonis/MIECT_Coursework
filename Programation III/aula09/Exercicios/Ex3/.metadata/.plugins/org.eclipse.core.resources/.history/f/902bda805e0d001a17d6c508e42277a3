package aula09Ex3;

public abstract class TesteIterador {
	public static void main(String[] args) {
		VectorPessoas vp = new VectorPessoas();
		for (int i=0; i<10; i++) {
			vp.addPessoa(new Pessoa("Bebé no Vector "+i, 1000+i, new Data()));
		}
		Iterator vec = vp.iterator();
		while ( vec.hasNext() ) {
			System.out.println( vec.next() );
		}
		System.out.println();
		ListaPessoasIterator lp = new ListaPessoasIterator();
		for (int i=0; i<10; i++) {
			lp.addPessoa(new Pessoa("Bebé na Lista "+i, 2000+i, new Data()));
		}
		Iterator lista = lp.iterator();
		while ( lista.hasNext() ) {
			System.out.println( lista.next() );
		}
	}
}
