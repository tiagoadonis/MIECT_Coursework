package aula06Ex1;

public class AlimentoVegetariano extends Alimento{
	private String nome;
	
	public AlimentoVegetariano(String nome, double prot, double cal, double pes) {
		super(prot,cal,pes);
		this.nome = nome;
	}
	
	@Override
	public String toString() {
		return this.nome+" (Alimento Vegetariano)"+super.toString();
	}
}
