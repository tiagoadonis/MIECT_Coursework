package aula03Ex2;

public class Figura {
	private Ponto centro;
	
	public Figura(Ponto centro) {
		this.centro = centro;
		System.out.println("NA FIGURA");
		System.out.println(""+centro.toString() );
		System.out.println("");
	}
	
	public Ponto getCentro() {
		return this.centro;
	}
	
	@Override
	public String toString() {
		return "Figura: \n"
				+ "Centro: "+this.getCentro();
	}
}
