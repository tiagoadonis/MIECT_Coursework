package aula03Ex1;

public class Pessoa {
	private String nome;
	private String cc;
	private Data nascimento;
	
	public Pessoa(String nome, String cc, Data nascimento) {
		this.nome = nome;
		this.cc = cc;
		this.nascimento = nascimento;
	}

	public String getNome() {
		return nome;
	}

	public String getCc() {
		return cc;
	}

	public Data getNascimento() {
		return nascimento;
	}

	@Override
	public String toString() {
		return nome+", cc:" +cc+ ", ("+nascimento+")";
	}
	
}
