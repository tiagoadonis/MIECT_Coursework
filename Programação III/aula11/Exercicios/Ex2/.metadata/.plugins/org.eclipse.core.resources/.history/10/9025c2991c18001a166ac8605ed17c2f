package aula11Ex2;
import java.util.*;

public class TestsMain {
	public static void main(String[] args) {
		
	}
	private static Figura maiorFiguraJ7(List<Figura> figs) {
		Figura maior = figs.get(0);
		for (Figura f : figs) {
			if (f.compareTo(maior) >= 1) {
				maior = f;
			}
		}
		return maior;
	}
}
